{
  "name": "davidhirtz/yii2-cms-parent",
  "type": "yii2-extension",
  "description": "Yii2 extension for yii2-cms that enables entry hierachy",
  "homepage": "https://www.davidhirtz.com/",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "davidhirtz/yii2-cms": ">=1.1.5"
  },
  "autoload": {
    "psr-4": {
      "davidhirtz\\yii2\\cms\\parent\\": ""
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:davidhirtz/yii2-cms.git"
    }
  ]
}