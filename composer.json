{
  "name": "davidhirtz/yii2-cms-parent",
  "type": "yii2-extension",
  "description": "Yii2 extension for yii2-cms that enables entry hierachy",
  "homepage": "https://www.davidhirtz.com/",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "davidhirtz/yii2-cms": ">=1.2.11"
  },
  "autoload": {
    "psr-4": {
      "davidhirtz\\yii2\\cms\\parent\\": ""
    }
  },
  "extra": {
    "bootstrap": "davidhirtz\\yii2\\cms\\parent\\composer\\Bootstrap"
  }
}